//https://www.interviewbit.com/problems/noble-integer/

int Solution::solve(vector<int> &A) {
    sort(A.begin(),A.end());
    int n=A.size();
   // vector<int>b;
  /* for(int i=0;i<n;i++)
    cout<<A[i]<<" ";
   cout<<endl;*/
   for(int i=0;i<n;i++){
       int j=i+1;
       while(j<n&&A[j]==A[i])
        j++;
      // cout<<j<<endl;
       if(A[i]>=0&&(n-j)==A[i])
        return 1;
   }
   return -1;
}
